{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "substitutions"
  ],
  "properties": {
    "substitutions": {
      "$id": "#/properties/substitutions",
      "type": "array",
      "title": "The Substitutions Schema",
      "items": {
        "$id": "#/properties/substitutions/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "tpnb"
        ],
        "properties": {
          "tpnb": {
            "$id": "#/properties/substitutions/items/properties/tpnb",
            "type": "string",
            "title": "The Tpnb Schema",
            "default": "",
            "examples": [
              "11111111"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}
